{
  "name": "node-tools",
  "version": "0.0.0",
  "scripts": {
    "sass": "npx sass --style=compressed ../../src/scss/style.scss",
    "autoprefixer": "./node_modules/postcss-cli/bin/postcss -u autoprefixer --autoprefixer.browsers '> 1%' -o ../../src/static/s/css/style.min.css --no-map",
    "build-css": "npm run sass | npm run autoprefixer"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "postcss-cli": "^7.1.0",
    "sass": "^1.26.3"
  }
}
